{
  "name": "byjg/micro-orm",
  "description": "A micro framework for create a very simple decoupled ORM (sqlite, mysql, postgres, sqlserver)",
  "autoload": {
    "psr-4": {
      "ByJG\\MicroOrm\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "byjg/anydataset-db": "5.0.0.x-dev"
  },
  "require-dev": {
    "phpunit/phpunit": "5.7.*|7.4.*|^9.5"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*"
  },
  "license": "MIT"
}

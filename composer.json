{
  "name": "byjg/micro-orm",
  "description": "A micro framework for create a very simple decoupled ORM (sqlite, mysql, postgres, sqlserver)",
  "autoload": {
    "psr-4": {
      "ByJG\\MicroOrm\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1 <8.5",
    "ext-json": "*",
    "byjg/anydataset-db": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5|^11.5",
    "byjg/cache-engine": "^6.0",
    "vimeo/psalm": "^5.9|^6.2"
  },
  "suggest": {
    "ext-curl": "*",
    "ext-pdo_mysql": "*"
  },
  "license": "MIT"
}
